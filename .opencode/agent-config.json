{
  "agents": {
    "coder": {
      "command": "opencode",
      "args": ["run", "{prompt}", "--agent", "coder"],
      "description": "Default OpenCode agent for code generation"
    },
    "augment": {
      "command": "augment",
      "args": ["code", "--prompt", "{prompt}"],
      "description": "A hypothetical CLI for augmenting code"
    },
    "cursor": {
      "command": "cursor",
      "args": ["--task", "{prompt}"],
      "description": "Cursor AI agent example"
    }
  }
}
